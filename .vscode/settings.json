{
  "workbench.colorTheme": "One Dark Pro",
  "liveServer.settings.donotShowInfoMsg": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.fontFamily": "'Cascadia Code',Monaco",
  "editor.fontLigatures": true,
  "editor.minimap.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.fontFamily": "monospace",
  "liveServer.settings.donotVerifyTags": true,
  "window.zoomLevel": -1,
  "editor.tabSize": 2,
  "editor.unicodeHighlight.nonBasicASCII": false,
  "cSpell.language": "en,es",
  "markdown-pdf.headerTemplate": "<div style=\"font-size: 9px; margin-left: 1cm;\"> </div> <div style=\"font-size: 9px; margin-left: auto; margin-right: 1cm; \"> </div>",
  "[json]": {
      "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[javascript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorCustomizations": {
      "scrollbarSlider.background": "#79550a",
      "scrollbarSlider.hoverBackground": "#d0930f",
      "scrollbarSlider.activeBackground": "#ff0000"
  }
}